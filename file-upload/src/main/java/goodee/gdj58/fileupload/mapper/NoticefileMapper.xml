<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="goodee.gdj58.fileupload.mapper.NoticefileMapper">
<insert id="insertNoticefile" parameterType="goodee.gdj58.fileupload.vo.Noticefile">
		insert into noticefile (
			notice_id
			,noticefile_name
			,noticefile_type
			,noticefile_size
		) values (
			#{noticeId}
			,#{noticefileName}
			,#{noticefileType}
			,#{noticefileSize}
		)
	</insert>
</mapper>